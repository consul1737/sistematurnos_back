{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": "> 0.25%, not dead",
            "useBuiltIns": "usage",
            "corejs": 3
          }
        ]
      ]
    },
    "development": {
      "presets": [
        "@babel/preset-env"
      ]
    }
  }
}
